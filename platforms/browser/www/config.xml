<?xml version='1.0' encoding='utf-8'?>
<widget id="com.juhastenroos.lunchlottery" version="2.3.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <name>Helsinki Lunch Lottery</name>
    <description>
        Application picks a random lunchplace based on users location.
    </description>
    <author email="juha@stenroos.net" href="http://juhastenroos.fi">
        Juha Stenroos
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access subdomains="true" uri="https://*.firebaseio.com" />
    <access origin="http://maps.google.com" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <universal-links>
        <host name="https://rzbj6.app.goo.gl&#xA;" scheme="https" />
        <host name="helsinkilunchlottery.firebaseapp.com" scheme="https">
            <path url="/__/auth/callback" />
        </host>
    </universal-links>
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="AndroidLaunchMode" value="singleTask" />
</widget>
